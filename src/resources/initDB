
CREATE TABLE  companies
(ID INT AUTO_INCREMENT PRIMARY KEY,
 NAME VARCHAR(50)
);

CREATE TABLE  customers
(ID INT AUTO_INCREMENT PRIMARY KEY,
 NAME VARCHAR(50)
);

CREATE TABLE  developers
(ID INT AUTO_INCREMENT PRIMARY KEY,
 NAME VARCHAR(50)
);

CREATE TABLE  project
(ID INT AUTO_INCREMENT PRIMARY KEY,
 NAME VARCHAR(50)
);

CREATE TABLE  skills
(ID INT AUTO_INCREMENT PRIMARY KEY,
 NAME VARCHAR(50)
);

CREATE TABLE client_project
(client_name VARCHAR(50),
 project_name VARCHAR(50),
  constraint PRIMARY KEY(client_name, project_name)
);

CREATE TABLE companies_projects
(companies_name VARCHAR(50),
 projects_name VARCHAR(50),
  constraint PRIMARY KEY(companies_name, project_name)
);

CREATE TABLE developers_project
(developers_name VARCHAR(50),
 project_name VARCHAR(50),
  constraint PRIMARY KEY(developers_name, project_name)
);

CREATE TABLE developers_skills
(developers_name VARCHAR(50),
 skills_name VARCHAR(50),
  constraint PRIMARY KEY(developers_name, skills_name)
)
